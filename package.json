{
    "name": "keleya",
    "version": "1.0.0",
    "description": "Keleya Challenge Test",
    "main": "index.js",
    "ava": {
        "compileEnhancements": false,
        "extensions": [
            "ts"
        ],
        "require": [
            "ts-node/register"
        ]
    },
    "scripts": {
        "test": "tsc && ava",
        "start:ts": "npx tsc --watch",
        "start:server": "nodemon dist/index.js",
        "start:dev": "concurrently 'npm run start:ts' 'npm run start:server'",
        "clean-install": "rm -rf node_modules package-lock.json && npm install",
        "create-db": "mysql -u root -p mysql < src/constants/db-init.sql",
        "fake-data": "mysql -u root -p mysql < src/constants/fake-data.sql"
    },
    "author": "Sam Barros",
    "license": "ISC",
    "dependencies": {
        "axios": "^0.18.0",
        "bcrypt": "^3.0.4",
        "body-parser": "^1.18.3",
        "concurrently": "^4.1.0",
        "cookie-parser": "^1.4.4",
        "cors": "^2.8.5",
        "dotenv": "^6.2.0",
        "express": "^4.16.4",
        "express-validator": "^5.3.1",
        "jsonwebtoken": "^8.4.0",
        "mysql2": "^1.6.5",
        "nodemon": "^1.18.10",
        "sequelize": "^5.15.1"
    },
    "devDependencies": {
        "@types/bcrypt": "^3.0.0",
        "@types/body-parser": "^1.17.0",
        "@types/cookie-parser": "^1.4.1",
        "@types/cors": "^2.8.4",
        "@types/es6-promise": "^3.3.0",
        "@types/express": "^4.16.1",
        "@types/express-validator": "^3.0.0",
        "@types/jsonwebtoken": "^8.3.0",
        "@types/node": "^11.9.4",
        "@types/sequelize": "^4.27.36",
        "ava": "^1.2.1",
        "ts-node": "^8.0.2",
        "typescript": "^3.3.3"
    }
}
